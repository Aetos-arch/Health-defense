                     <image>
                        <Image url="@../../Sources/Tourelles/TourelleVitamine.png" />
                     </image>
                  </ImageView>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="150.0" text="Tourelle vitamine 500" />
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="150.0">
               <children>
                  <ImageView fx:id="tourelleSeringue" fitHeight="32.0" fitWidth="32.0" onDragDetected="#onDragDetected" pickOnBounds="true" preserveRatio="true">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <image>
                        <Image url="@../../Sources/Tourelles/tourelleSeringue.png" />
                     </image>
                  </ImageView>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="150.0" text="Tourelle seringue 750" />
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="150.0">
               <children>
                  <ImageView fx:id="tourelleVaccin" fitHeight="32.0" fitWidth="32.0" onDragDetected="#onDragDetected" pickOnBounds="true" preserveRatio="true">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <image>
                        <Image url="@../../Sources/Tourelles/TourelleVaccin.png" />
                     </image>
                  </ImageView>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="150.0" text="Tourelle vaccin 1000" />
               </children></VBox>
            <VBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="150.0">
               <children>
                    <Button alignment="TOP_LEFT" mnemonicParsing="false" onAction="#action" prefHeight="100.0" prefWidth="150.0" styleClass="bouton" text="Vague suivante" textAlignment="CENTER" textOverrun="CLIP" wrapText="true">
                     <cursor>
                        <Cursor fx:constant="DEFAULT" />
                     </cursor>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
   </bottom>
   <right>
      <VBox maxHeight="150.0" prefHeight="150.0" prefWidth="150.0" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="CENTER" maxHeight="480.0" maxWidth="200.0" minHeight="480.0" minWidth="200.0" styleClass="infoLabel">
