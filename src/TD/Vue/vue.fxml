                  <Label text="Vague" />
                   <Label fx:id="labelVague" />
                  <Label text="Score" />
                   <Label fx:id="labelScore" />
                  <Label text="Money" />
                   <Label fx:id="labelMoney" />
                  <Label text="PV" />
                   <Label fx:id="labelPV" />
               </children>
            </VBox>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </right>
   <left>
      <VBox maxHeight="480.0" maxWidth="200.0" minHeight="480.0" minWidth="200.0" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
