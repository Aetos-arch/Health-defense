      <Label fx:id="labelInfo" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <HBox alignment="CENTER" minHeight="120.0" prefHeight="120.0" prefWidth="1200.0" BorderPane.alignment="CENTER">
